{
    "name": "madeinua/laravel-dripflow",
    "description": "Content drip-feed mechanism for Laravel with polymorphic relationships and flexible unlock strategies",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "dripflow",
        "drip-feed",
        "content-unlock",
        "scheduled-content"
    ],
    "authors": [
        {
            "name": "Artem Parkhomov",
            "email": "madeinua@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "MadeInUA\\LaravelDripFlow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadeInUA\\LaravelDripFlow\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadeInUA\\LaravelDripFlow\\DripFlowServiceProvider"
            ],
            "aliases": {
                "DripFlow": "MadeInUA\\LaravelDripFlow\\Facades\\DripFlow"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}